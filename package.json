{
  "name": "provider-proxy",
  "version": "0.1.0",
  "description": "Cloudflare worker to load balancer provider endpoints",
  "main": "dist/worker.js",
  "scripts": {
    "build": "esbuild --bundle --outfile=dist/worker.js --minify --sourcemap src/index.ts",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}'",
    "test": "bash test.sh"
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@cloudflare/wrangler": "^1.19.0",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "esbuild": "^0.12.24",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-typescript": "^3.0.0",
    "prettier": "^2.3.0",
    "service-worker-mock": "^2.0.5",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2"
  }
}
